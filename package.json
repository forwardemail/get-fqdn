{
  "name": "get-fqdn",
  "description": "Lookup the fully qualified domain name (\"FQDN\") of the current server's IP (default) or a custom IP.  90x faster than `hostname -f` and works with Node v6.4+.",
  "version": "0.0.4",
  "author": "Nick Baugh <niftylettuce@gmail.com> (http://niftylettuce.com/)",
  "bugs": {
    "url": "https://github.com/forwardemail/get-fqdn/issues",
    "email": "niftylettuce@gmail.com"
  },
  "contributors": [
    "Nick Baugh <niftylettuce@gmail.com> (http://niftylettuce.com/)"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "ava": "^5.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "eslint-config-xo-lass": "^2.0.1",
    "fixpack": "^4.0.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.0",
    "nyc": "^15.1.0",
    "remark-cli": "^11.0.0",
    "remark-preset-github": "^4.0.4",
    "rimraf": "^4.0.4",
    "xo": "^0.53.1"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/forwardemail/get-fqdn",
  "keywords": [
    "address",
    "dns",
    "domain",
    "domains",
    "fetch",
    "fqdn",
    "fully",
    "get",
    "host",
    "hostname",
    "hostnames",
    "ip",
    "ipv4",
    "ipv6",
    "lookup",
    "machine",
    "name",
    "os",
    "qualifie",
    "qualified",
    "qualify"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/forwardemail/get-fqdn"
  },
  "scripts": {
    "lint": "xo --fix && remark . -qfo && fixpack",
    "prepare": "husky install",
    "pretest": "npm run lint",
    "test": "npm run test-coverage",
    "test-coverage": "cross-env NODE_ENV=test nyc ava"
  }
}
